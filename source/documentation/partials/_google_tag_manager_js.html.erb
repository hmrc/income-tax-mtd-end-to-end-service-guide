<% content_for :head do %>

  <%
    tracking_url = ENV['ENVIRONMENT'] == 'local' ?
      config[:tech_docs][:tracking_consent_frontend][:local_url] :
      config[:tech_docs][:tracking_consent_frontend][:remote_url]
  %>

  <!--[if !IE]>-->
  <script
    src="<%= tracking_url %>"
    id="tracking-consent-script-tag"
    data-gtm-container="<%= config[:tech_docs][:tracking_consent_frontend][:gtm_container] %>"
    data-language="en"
  ></script>
  <!--<![endif]-->
<% end %>